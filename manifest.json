{
	"manifest_version": 2,
	"version": "1.3.0",
	"name": "YT video screenshot addon",
	"description": "on the YT video settings, add a Screenshot button",
	"author": "san.d",
	"icons": {
        "32": "icons/logo_32.png",
        "64": "icons/logo_64.png"
    },
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*"],
			"js": ["yt_screensort.js"],
			"run_at": "document_end"
		}
	]
}
